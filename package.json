{
  "name": "my-project",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "me",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ICU_DATA=./node_modules/full-icu && nuxt",
    "build": "cross-env NODE_ICU_DATA=./node_modules/full-icu && nuxt build",
    "start": "cross-env NODE_ICU_DATA=./node_modules/full-icu && nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "cross-env PRE_COMMIT=true lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,vue,css}": [
      "eslint --fix --ignore-path .gitignore src",
      "git add"
    ]
  },
  "dependencies": {
    "nuxt": "npm:nuxt-edge",
    "typeface-aleo": "0.0.44",
    "typeface-clear-sans": "0.0.44",
    "vue-agile": "^0.3.7",
    "vue-mq": "^0.2.1"
  },
  "devDependencies": {
    "full-icu": "^1.2.1",
    "babel-eslint": "^8.2.3",
    "cross-env": "^5.1.4",
    "eslint": "^4.15.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-plugin-vue": "^4.5.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.5",
    "prettier": "^1.12.1"
  }
}
